module Anthropic
  module Models
    type citations_delta =
      {
        citation: Anthropic::Models::CitationsDelta::citation,
        type: :citations_delta
      }

    class CitationsDelta < Anthropic::BaseModel
      attr_accessor citation: Anthropic::Models::CitationsDelta::citation

      attr_accessor type: :citations_delta

      def initialize:
        (
          citation: Anthropic::Models::CitationsDelta::citation,
          type: :citations_delta
        ) -> void
        | (
          ?Anthropic::Models::citations_delta | Anthropic::BaseModel data
        ) -> void

      def to_h: -> Anthropic::Models::citations_delta

      type citation =
        Anthropic::Models::CitationCharLocation
        | Anthropic::Models::CitationPageLocation
        | Anthropic::Models::CitationContentBlockLocation

      class Citation < Anthropic::Union
        private def self.variants: -> [[:char_location, Anthropic::Models::CitationCharLocation], [:page_location, Anthropic::Models::CitationPageLocation], [:content_block_location, Anthropic::Models::CitationContentBlockLocation]]
      end
    end
  end
end
