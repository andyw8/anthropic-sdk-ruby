module Anthropic
  module Models
    type error_object =
      Anthropic::Models::InvalidRequestError
      | Anthropic::Models::AuthenticationError_
      | Anthropic::Models::BillingError
      | Anthropic::Models::PermissionError
      | Anthropic::Models::NotFoundError_
      | Anthropic::Models::RateLimitError_
      | Anthropic::Models::GatewayTimeoutError
      | Anthropic::Models::APIErrorObject
      | Anthropic::Models::OverloadedError

    class ErrorObject < Anthropic::Union
      def self.variants: -> [Anthropic::Models::InvalidRequestError, Anthropic::Models::AuthenticationError_, Anthropic::Models::BillingError, Anthropic::Models::PermissionError, Anthropic::Models::NotFoundError_, Anthropic::Models::RateLimitError_, Anthropic::Models::GatewayTimeoutError, Anthropic::Models::APIErrorObject, Anthropic::Models::OverloadedError]
    end
  end
end
