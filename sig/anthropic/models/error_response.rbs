module Anthropic
  module Models
    type error_response =
      { error: Anthropic::Models::error_object, type: :error }

    class ErrorResponse < Anthropic::BaseModel
      attr_accessor error: Anthropic::Models::error_object

      attr_accessor type: :error

      def initialize:
        (error: Anthropic::Models::error_object, type: :error) -> void
        | (
          ?Anthropic::Models::error_response | Anthropic::BaseModel data
        ) -> void

      def to_h: -> Anthropic::Models::error_response
    end
  end
end
